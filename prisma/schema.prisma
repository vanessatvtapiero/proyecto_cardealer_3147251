generator client {
  provider = "prisma-client-js"
  output   = "../generated/prisma"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}
model alertas {
  id  Int @id @default(autoincrement())
  model String
  plate String
  release_date DateTime @default(now())
  is_available Boolean @default(true) 
  comentarios comentarios[]
}
model comentarios {
  id  Int @id @default(autoincrement())
  descripcion String
  plate String
  release_date DateTime @default(now())
  is_available Boolean @default(true) 
  alerta_id Int
  alerta alertas @relation(fields: [alerta_id], references: [id])
}

